    </script>

    <!-- Gen. Secretary -->
    <script>
      const myVar2 = <%- JSON.stringify(gs_name); %>;
        const voteNo2 = <%- JSON.stringify(gs_vote); %>;
        var ctx2 = document.getElementById('barChart2');
        var myChart = new Chart(ctx2, {
            type: 'bar',
            data: {
